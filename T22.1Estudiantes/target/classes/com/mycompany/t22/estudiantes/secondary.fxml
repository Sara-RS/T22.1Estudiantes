<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.t22.estudiantes.SecondaryController">
   <children>
        <Label text="Tabla Estudiantes" />
      <TableView fx:id="tblestudiantes" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="colNom" prefWidth="75.0" text="Nombre" />
          <TableColumn fx:id="colApe" prefWidth="75.0" text="Apellido" />
            <TableColumn fx:id="colFeN" prefWidth="126.0" text="FechaNacimiento" />
        </columns>
      </TableView>
        <Button id="btnSalir" fx:id="secondaryButton" onAction="#switchToPrimary" text="Mostrar Estudiantes" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
